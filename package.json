{
  "name": "movie-app-react",
  "version": "1.0.0",
  "author": "Nikhil jahagirdar",
  "email": "nikhil.jahagirdar@gmail.com",
  "description": "Allows to search and browse Movies from The Movie Db API",
  "scripts": {
    "open:src": "babel-node tools/serverDev.js",
    "open:docs": "babel-node tools/serverDocs.js",
    "build-docs": "rimraf docs && jsdoc -c jsDoc.json",
    "docs": "npm run build-docs && npm run open:docs",
    "lint": "esw --ext .js --ext .jsx webpack.config.* common src tools",
    "test": "jest --coverage",
    "check": "npm-run-all test lint",
    "check:watch": "npm-run-all lint:watch test:watch",
    "lint:watch": "npm run lint -- --watch",
    "test:watch": "npm run test -- --watchAll",
    "start": "npm-run-all --parallel open:src",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm-run-all clean-dist",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/serverProd.js",
    "ultimate": "npm install && npm run build"
  },
  "license": "ISC",
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-helper-builder-react-jsx": "~6.26.0",
    "babel-helper-regex": "~6.26.0",
    "babel-loader": "~7.1.2",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-1": "~6.24.1",
    "babel-register": "~6.26.0",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "css-loader": "~0.28.11",
    "express": "~4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "~3.2.0",
    "less": "~3.9.0",
    "less-loader": "~4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prop-types": "~15.6.0",
    "react-svg-loader": "^2.1.0",
    "redux-devtools-extension": "^2.13.7",
    "style-loader": "~0.19.1",
    "url-loader": "~1.1.2",
    "webpack": "~4.26.1",
    "webpack-dev-server": "~3.1.10"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "bootstrap": "^4.2.1",
    "classnames": "~2.2.6",
    "jquery": "^3.3.1",
    "popper": "^1.0.1",
    "react": "~16.6.3",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "~16.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "~4.3.1",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3"
  }
}
